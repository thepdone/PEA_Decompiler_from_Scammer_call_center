<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:angle="0"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#ffdddddd" android:endColor="#ffdddddd" android:angle="0" android:centerColor="#ffdddddd"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff3d8826" android:endColor="#ff3d8826" android:angle="0" android:centerColor="#ff3d8826"/>
            </shape>
        </clip>
    </item>
</layer-list>
